<div>
    <label for="newField">New Field:</label>
    <input type="text" id="newField" valueLink="{this.linkState('newField')}"/>
    <button onClick="{this.addField}">Add</button>
    <form>
        <div rt-repeat="field in this.state.fields">
            <label for="{field}">{field}: </label>
            <input type="text" id="{field}" onChange="{this.logChange}"/>
        </div>
    </form>
</div>

